@model Prontuario

@{
    ViewData["Title"] = "Visualiza";
}

<h1>Detalhes do Prontuário</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            CPF
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CPF)
        </dd>
        <dt class="col-sm-2">
            Convênio
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Convenio)
        </dd>
        <dt class="col-sm-2">
            Plano
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Plano)
        </dd>
        <dt class="col-sm-2">
            Telefone
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Telefone)
        </dd>
    </dl>
</div>
<div class="form-group">
    <a class="btn btn-outline-info" asp-action="Index">Voltar</a>
</div>
